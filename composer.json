{
    "name": "ruwork/polyfill-form-dti-bundle",
    "description": "Symfony Form polyfill bundle adding input=datetime_immutable to date types.",
    "type": "symfony-bundle",
    "keywords": ["symfony", "form", "polyfill", "bundle"],
    "license": "MIT",
    "authors": [
        {"name": "Valentin Udaltsov", "email": "udaltsov.valentin@gmail.com"}
    ],
    "require": {
        "php": ">=5.5",
        "ruwork/polyfill-form-dti": "^0.1@dev",
        "symfony/dependency-injection": "^2.8 || ^3.0 || ^4.0",
        "symfony/form": "^2.8 || ^3.0 || ^4.0",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^1.2",
        "symfony/phpunit-bridge": "^2.8 || ^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\PolyfillFormDTIBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
